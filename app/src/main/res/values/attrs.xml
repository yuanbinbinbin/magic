<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--ShimmerView Start-->
    <declare-styleable name="ShimmerView">
        <attr name="reflectionColor" format="color" />
    </declare-styleable>
    <!--ShimmerView End-->

    <!--ShapeImageView Start-->
    <declare-styleable name="ShapeImageView">
        <attr name="siv_left_top_corner_radius" format="dimension" />
        <attr name="siv_right_top_corner_radius" format="dimension" />
        <attr name="siv_left_bottom_corner_radius" format="dimension" />
        <attr name="siv_right_bottom_corner_radius" format="dimension" />
        <attr name="siv_border_width" format="dimension" />
        <attr name="siv_border_color" format="color" />
        <attr name="siv_oval" format="boolean" />
        <attr name="android:scaleType" />
    </declare-styleable>
    <!--ShapeImageView End-->

</resources>